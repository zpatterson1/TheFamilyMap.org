    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("The Family Map Inventories", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                <header>
                    <div id="hdr">
                        <div id="logo">
                            <a href="http://www.thefamilymap.org" title="The Family Map"><img src="http://www.thefamilymap.org/wp-content/themes/thefamilymap/images/logo.png" alt="The Family Map"></a>
                        </div>
                        <div id="login">
                            <div id="loginform">
                                <h4>Members Login</h4>
                                <div id="login_message"></div>
                                <form id="member_login">
                                    <input type="text" id="username" name="username" value="username" /> <input type="password" id="password" name="password" value="password" /> <input type="submit" class="btn" />
                                </form>
                                <a id="forgot_password" href="#">Forgot your password?</a>
                            </div>
                        </div>
                        </header>
                        <div class="navbar-collapse collapse">
                            <ul class="nav navbar-nav">

                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                <li>@Html.ActionLink("Overview", "overview", "Home")</li>
                                <li>@Html.ActionLink("How it Works", "HowitWorks", "Home")</li>
                                <li>@Html.ActionLink("Training", "Training", "Home")</li>
                                <li>@Html.ActionLink("Overview", "Overview", "Home")</li>
                            </ul>
                        </div>
                    </div>
            </div>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - The Family Map</p>
            </footer>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
